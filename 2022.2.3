#property装饰器的使用
class Student:
    __school='ahu'
    def __init__(self,name,age):
        self.name=name
        self.__age=age
    @property
    def age(self):
        return self.__age
    @age.setter
    def age(self,age):
        self.__age=age
s1=Student('dsq',20)
s1.age=2000
print(s1.age)
#自然
class Student:
    school='ahu'
    def __init__(self,name):
        self.__name=name
    def get_name(self):
        return self.__name
    def set_name(self,name):
        self.__name=name
s1=Student('胡超')
s1.set_name('dsq')
print(s1.get_name())
